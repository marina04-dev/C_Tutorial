#include<stdio.h>

int main()
{
    int pinakas[5];
    int i;
    int gin;
    for (i=0; i<5; i++)  {
        do {
            printf("Eisagete ton %d-o arithmo ", i+1);
            scanf("%d", &pinakas[i]);
        } while (pinakas[i]<1 || pinakas[i]>8)
    }
    
    gin=1;
    for (i=0; i<5; i++) {
        gin=gin*pinakas[i];
    }
    printf("To ginomeno twn arithwn einai: %d", gin);
}
